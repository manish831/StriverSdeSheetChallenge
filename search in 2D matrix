bool searchMatrix(vector<vector<int>>& mat, int tgt) {
        int rows = mat.size(), col = mat[0].size();

        int i = 0, j = col-1;

        while(i<rows and j>=0){
            if(mat[i][j] == tgt){
                return true;
            }
            if(mat[i][j] < tgt){
                i++;
            }
            else{
                j--;
            }
        }
        return false;
}
